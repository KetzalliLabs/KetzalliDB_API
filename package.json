{
  "name": "ketzallidb-api",
  "version": "1.0.0",
  "description": "CRUD Express API with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rmdir /s /q dist 2>nul || true",
    "admin:create": "tsx src/scripts/createFirstAdmin.ts",
    "admin:set": "tsx src/utils/setAdmin.ts set",
    "admin:remove": "tsx src/utils/setAdmin.ts remove",
    "admin:list": "tsx src/utils/setAdmin.ts list"
  },
  "keywords": [
    "express",
    "typescript",
    "crud",
    "api",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.930.0",
    "@aws-sdk/s3-request-presigner": "^3.930.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.6.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.24",
    "@types/pg": "^8.15.6",
    "@types/sharp": "^0.31.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
